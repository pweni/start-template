{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "preserve",
    "experimentalDecorators": true,
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "strictPropertyInitialization": false
  },
  "include": ["**/*.ts"]
}
